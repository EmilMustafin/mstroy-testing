{
    "name": "mstroy-testing",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "concurrently \"npm run type-check\" \"npm run build-only\"",
        "build-only": "vite build",
        "preview": "vite preview",
        "type-check": "vue-tsc",
        "lint:format": "prettier --cache --check .",
        "lint:style": "stylelint --max-warnings 0 --ignore-path .gitignore \"**/*.{css,scss,vue,vuex}\"",
        "lint": "eslint . --fix",
        "lint:all": "concurrently \"npm run type-check\" \"npm run lint\" \"npm run format\" \"npm run lint:style\"",
        "format": "prettier --write src/",
        "test:unit": "vitest",
        "update": "npx npm-check-updates -i"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.18",
        "ag-grid-community": "^35.0.0",
        "ag-grid-enterprise": "^35.0.0",
        "ag-grid-vue3": "^35.0.0",
        "vue": "^3.5.27",
        "vue-eslint-parser": "^10.2.0",
        "vue-router": "^5.0.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.2",
        "@tsconfig/node22": "^22.0.5",
        "@types/jsdom": "^27.0.0",
        "@types/node": "^25.2.0",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "@vitejs/plugin-vue": "^6.0.4",
        "@vitest/eslint-plugin": "^1.6.6",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.8.1",
        "autoprefixer": "^10.4.24",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-config-flat-gitignore": "^2.1.0",
        "eslint-plugin-vue": "^10.7.0",
        "jsdom": "^28.0.0",
        "prettier": "^3.8.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "tailwindcss": "^4.1.18",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "^7.3.1",
        "vite-plugin-checker": "^0.12.0",
        "vite-plugin-vue-devtools": "^8.0.5",
        "vitest": "^4.0.18",
        "vue-tsc": "^3.2.4"
    }
}
